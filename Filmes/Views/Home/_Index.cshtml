@model PagedList.IPagedList<Filmes.Models.DadosFilmes>
@using PagedList.Mvc


<div class="form-group input-group">
    <span class="btn input-group-addon"><i class="glyphicon glyphicon-search" onclick="FormPesquisa.submit()"></i></span>
    @Html.EditorFor(model => Model.FirstOrDefault().pesquisa, new { htmlAttributes = new { @class = "form-control" } })


</div>
<div id="div-lista">
    @Html.Partial("_ListaFilmes")
</div>

